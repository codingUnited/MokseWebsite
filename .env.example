# .env.example

# Docker Compose profile to use
COMPOSE_PROFILES=dev

# Build target stage in Dockerfile
BUILD_TARGET=dev

# Port your app runs on inside the container
APP_PORT=3000

# Node environment
NODE_ENV=development

# Default command to start the app
APP_CMD=npm run dev

# Bind mounts for development
DEV_MOUNT=.:/app
DEV_NODEMODULES_MOUNT=/app/node_modules
